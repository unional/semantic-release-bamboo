{
  "name": "semantic-release-bamboo",
  "description": "semantic-release plugin for Bamboo",
  "version": "0.0.0",
  "main": "dist-es5/index.js",
  "typings": "dist-es5/index.d.ts",
  "files": [
    "dist-es5",
    "src"
  ],
  "scripts": {
    "build": "npm run clean && npm run build-es5",
    "build-es5": "tsc -p tsconfig.es5.json",
    "clean": "rimraf dist dist-es5",
    "dependency-check": "dependency-check . --unused --no-dev && dependency-check . --missing --no-dev",
    "lint": "tslint -p tsconfig.json 'src/**/*.ts'",
    "prepublishOnly": "npm run build",
    "_test": "npm run clean && tsc && ava",
    ")verify": "npm run lint && npm run coverage && npm run build && npm run bundle && npm run dependency-check",
    "watch": "npm run clean && node scripts/watch.js",
    "semantic-release": "semantic-release"
  },
  "author": {
    "name": "Homa Wong",
    "email": "homawong@gmail.com"
  },
  "homepage": "https://github.com/unional/semantic-release-bamboo",
  "bugs": {
    "url": "https://github.com/unional/semantic-release-bamboo/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unional/semantic-release-bamboo.git"
  },
  "license": "MIT",
  "devDependencies": {
    "dependency-check": "^3.0.0",
    "rimraf": "^2.6.2",
    "semantic-release": "^12.4.1",
    "tslint": "^5.9.1",
    "tslint-config-unional": "^0.9.0",
    "typescript": "^2.7.1"
  },
  "dependencies": {
    "@semantic-release/error": "^2.1.0",
    "env-ci": "^1.4.0"
  }
}
